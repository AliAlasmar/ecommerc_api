                                                        "properties": {
                                                            "id": {
                                                                "description": "ID of the product",
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "name": {
                                                                "description": "Name of the product",
                                                                "type": "string",
                                                                "example": "Product Name"
                                                            },
                                                            "pivot": {
                                                                "properties": {
                                                                    "quantity": {
                                                                        "description": "Quantity ordered",
                                                                        "type": "integer",
                                                                        "example": 2
